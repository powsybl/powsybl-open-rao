	<cims:belongsToCategory rdf:resource="http://entsoe.eu/ns/CIM/Contingency-EU#Package_ContingencyProfile"/>
	<cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#enumeration"/>
