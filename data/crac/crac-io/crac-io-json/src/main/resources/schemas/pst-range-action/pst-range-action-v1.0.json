{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "$id": "resource:/schemas/pst-range-action/pst-range-action-v1.0.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": [
        "string",
        "null"
      ]
    },
    "operator": {
      "type": [
        "string",
        "null"
      ]
    },
    "groupId": {
      "type": [
        "string",
        "null"
      ]
    },
    "speed": {
      "type": "number"
    },
    "networkElementId": {
      "type": "string"
    },
    "initialTap": {
      "type": "number"
    },
    "tapToAngleConversionMap": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "ranges": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "rangeType": {
            "$ref": "../range-type/range-type-v1.0.json"
          },
          "max": {
            "type": "number"
          },
          "min": {
            "type": "number"
          }
        },
        "additionalProperties": false,
        "required": [
          "rangeType"
        ]
      }
    },
    "freeToUseUsageRules": {
      "type": "array",
      "items": {
        "$ref": "../free-to-use-usage-rule/free-to-use-usage-rule-v1.0.json"
      }
    },
    "onStateUsageRules": {
      "type": "array",
      "items": {
        "$ref": "../on-state-usage-rule/on-state-usage-rule-v1.0.json"
      }
    },
    "onFlowConstraintUsageRules": {
      "type": "array",
      "items": {
        "$ref": "../on-flow-constraint-usage-rule/on-flow-constraint-usage-rule-v1.0.json"
      }
    },
    "extensions": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "networkElementId",
    "ranges"
  ]
}
