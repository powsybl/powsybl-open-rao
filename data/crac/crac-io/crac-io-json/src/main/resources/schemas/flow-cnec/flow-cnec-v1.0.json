    "id": {
      "type": "string"
    },
    "name": {
      "type": [
        "string",
        "null"
      ]
    },
    "networkElementId": {
      "type": "string"
    },
    "operator": {
      "type": [
        "string",
        "null"
      ]
    },
    "instant": {
      "type": "string"
    },
    "contingencyId": {
      "type": [
        "string",
        "null"
      ]
    },
    "optimized": {
      "type": "boolean"
    },
    "monitored": {
      "type": "boolean"
    },
    "frm": {
      "type": "number"
    },
